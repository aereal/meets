{
  "name": "meets",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "author": "aereal <aereal@aereal.org>",
  "license": "MIT",
  "scripts": {
    "compile:ts": "browserify -d -o $npm_package_config_out_file -p [tsify -p .] $npm_package_config_main_file",
    "compile:ts:watch": "watchify -v -o $npm_package_config_out_file -p [tsify -p .] $npm_package_config_main_file",
    "server": "python -m SimpleHTTPServer $npm_package_config_port",
    "proxy-server": "nginx -g 'daemon off;' -p . -c etc/nginx.conf",
    "start": "run-p server proxy-server compile:ts:watch"
  },
  "config": {
    "port": 8001,
    "out_file": "dist/js/index.js",
    "main_file": "src/index.tsx"
  },
  "devDependencies": {
    "@types/react": "^16.0.7",
    "@types/react-dom": "^15.5.5",
    "@types/react-redux": "^5.0.9",
    "@types/redux-logger": "^3.0.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "flow-bin": "^0.47.0",
    "npm-run-all": "^4.0.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "tsify": "^3.0.3",
    "typescript": "^2.5.3",
    "watchify": "^3.9.0",
    "whatwg-fetch": "^2.0.3"
  }
}
